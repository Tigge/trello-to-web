{
  "folder": "output",
  "basename": "generated",
  "title": "Generated HTML",
  "template": "html/default.html",
  "template-section": "html/default-section.html",
  "features": {
    "width": 600,
    "lines": true,
    "labels": true,
    "premailer": true
  },
  "extensions": [
    "markdown.extensions.extra",
    "markdown.extensions.attr_list",
    "markdown.extensions.smarty",
    "markdown_smarttoc"
  ],
  "css": [
    "css/default-base.css",
    "css/lines.css",
    "css/labels.css",
    "css/images.css"
  ]
}